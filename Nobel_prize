ALTER TABLE LESLIE.BIRTHPLACE
 DROP PRIMARY KEY CASCADE;

DROP TABLE LESLIE.BIRTHPLACE CASCADE CONSTRAINTS;

CREATE TABLE LESLIE.BIRTHPLACE
(
  BIRTHDATE_ID  NUMBER,
  BIRTHPLACE    VARCHAR2(1 BYTE)
)
TABLESPACE SYSTEM
PCTUSED    40
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            FREELISTS        1
            FREELIST GROUPS  1
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
MONITORING;


CREATE UNIQUE INDEX LESLIE.BIRTHPLACE_PK ON LESLIE.BIRTHPLACE
(BIRTHDATE_ID)
LOGGING
TABLESPACE SYSTEM
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            FREELISTS        1
            FREELIST GROUPS  1
            BUFFER_POOL      DEFAULT
           );

ALTER TABLE LESLIE.BIRTHPLACE ADD (
  CONSTRAINT BIRTHPLACE_PK
  PRIMARY KEY
  (BIRTHDATE_ID)
  USING INDEX LESLIE.BIRTHPLACE_PK
  ENABLE VALIDATE);
  
